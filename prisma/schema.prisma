generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "mongodb"
}

type ProductsImages {
  main   String
  others String[]
}

model products {
  id             String         @id @default(auto()) @map("_id") @db.ObjectId
  category       String
  description    String
  images         ProductsImages
  price          Int
  qualifications String[]
  title          String
  urlSlug        String
}
